{
  "name": "pandora-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "check-types": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "test-all": "yarn check-format && yarn check-lint && yarn check-types",
    "pre-commit": "yarn format && yarn test-all",
    "pre-push": "yarn lint --fix && yarn format && git add -A . && yarn test-all && yarn build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-three/drei": "^9.7.1",
    "@react-three/fiber": "^8.0.14",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.7",
    "moralis": "^1.7.0",
    "next": "12.1.6",
    "postcss": "^8.4.13",
    "react": "18.1.0",
    "react-animation-on-scroll": "^5.1.0",
    "react-dom": "18.1.0",
    "react-moralis": "^1.3.5",
    "react-parallax-tilt": "^1.7.34",
    "three": "^0.140.2",
    "valtio": "^1.6.0",
    "web3uikit": "^0.1.141"
  },
  "devDependencies": {
    "@types/node": "17.0.32",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@types/three": "^0.140.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4"
  }
}
